// iterative factorial

int fact (int n) {
		  int i, r = 2;
  i = 1 ;
  r = 1 ;
  while ((1 < 2) && true || false && (i < r) || (i + 1 < r) && !false) {
    r = r * i ;
    i++ ;
  }

	  int y = 5;
	  printInt(i+y);

  return r ;
}

// recursive factorial

int factr (int n) {
  if (n < 2)
    return 1 ;
  else
    return n * factr(n-1) ;
}

